<div class="new_vacation_link">
  <a href='<%= new_vacation_path %>'><h5>Create a vacation</h5></a>
</div>

<section class"section_one">
  <div class="body_one row">
    <div class="chat_box col-md-6"><h1>Chat box</h1></div>
    <div class="places_been col-md-6">
      <img src="http://maps.google.com/maps/api/staticmap?center=33.993082,13.160571&zoom=1&size=550x300&sensor=TRUE_OR_FALSE" alt="Google static world map">
    </div>
  </div>
</section>
<h4>Home city</h4>

<section class"section_two">
  <div class="body_two row">
    <div class="chat_box col-md-6"><h1>home city image and map</h1></div>
    <div class="places_been col-md-6">
    <iframe id="forecast_embed" class="weather_iframe" type="text/html" frameborder="0" height="60" width="100%" src="http://forecast.io/embed/#lat=<%= @profile.latitude %>&lon=<%= @profile.longitude %>&name=<%= @profile.city %>"> </iframe>
      <hr>
    </div>
  </div>
</section>

<h4>Saved Vacation</h4>

<section class"section_three">
  <div class="body_three row">
    <div class="chat_box col-md-3"><h1>Saved vacation</h1></div>
    <div class="places_been col-md-3"><h1>Saved vacation</h1></div>
    <div class="places_been col-md-3"><h1>Saved vacation</h1></div>
    <div class="places_been col-md-3"><h1>Saved vacation</h1></div>
  </div>
</section>

